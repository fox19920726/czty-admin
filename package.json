{
  "name": "czty-admin",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "czty-build dev",
    "start": "czty-build start",
    "pub": "czty-build pub"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fox19920726/czty-admin.git"
  },
  "author": "Jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fox19920726/czty-admin/issues"
  },
  "homepage": "https://github.com/fox19920726/czty-admin#readme",
  "devDependencies": {
    "@babel/register": "^7.7.4",
    "czty-eslint": "0.0.6",
    "czty-script": "^0.5.0"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "element-ui": "^2.12.0",
    "js-cookie": "^2.2.1",
    "js-md5": "^0.7.3",
    "mockjs": "^1.0.1-beta3",
    "nprogress": "^0.2.0",
    "qs": "^6.9.0",
    "regenerator-runtime": "^0.13.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential",
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "no-tabs": 0,
      "prefer-rest-params": 0,
      "import/no-unresolved": 0,
      "no-param-reassign": 0,
      "prefer-destructuring": 0,
      "linebreak-style": 0,
      "no-restricted-syntax": 0
    }
  },
  "postcss": {
    "syntax": "postcss-scss",
    "parser": "postcss-scss",
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 1 version",
    "maintained node versions",
    "not dead",
    "defaults",
    "ie > 6",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
